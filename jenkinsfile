node('jdk11-mvn3.8.5') {
    stage('git') {
        git 'https://github.com/Shri-1991/java11-examples.git'
    }
    stage('build') {
        sh '''
            echo "PATH=${PATH}"
            echo "M2_HOME=${M2_HOME}
        
        '''
        sh 'mvn clean package'    
    }
    stage('archive'){
        archive 'target/*.jar'
    }
} 